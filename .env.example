# Saleor
SALEOR_URL=https://<YOUR INSTANCE>.eu.saleor.cloud

# Logging
LOG_LEVEL=debug

# AWS Secrets Manager
AWS_ACCESS_KEY_ID=test
AWS_SECRET_ACCESS_KEY=test
AWS_SESSION_TOKEN=
AWS_REGION=us-east-1
SECRET_MANAGER_APP_CONFIG_PATH=saleor-app-config

# Development: Saleor Dashboard token for standalone mode
SALEOR_UI_APP_TOKEN=

# Localstack (for local development)
AWS_ENDPOINT_URL=http://localhost:4566

# Optional: URL prefix for the app
BASE_PATH=

# Server port (default: 3000)
PORT=3000
